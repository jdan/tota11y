    "handlebars-loader": "^1.0.0",
    "jquery": "^2.1.3",
    "jsdom": "^8.1.0",
    "less": "^2.5.0",
    "less-loader": "^2.2.0",
    "mocha": "^2.2.5",
    "postcss": "^4.1.11",
    "postcss-loader": "^0.4.3",
